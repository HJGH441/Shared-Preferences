<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ListaGamesActivity">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabPreferencias"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end|bottom"
        android:layout_marginRight="170dp"
        android:layout_marginBottom="16dp"
        android:contentDescription="Preferencias"
        android:src="@drawable/preferencias"
        app:backgroundTint="#D6CECE" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/RecyclerJuegos"
        tools:listitem="@layout/activity_segunda_vista"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </androidx.recyclerview.widget.RecyclerView>




</FrameLayout>